  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/react-dom": "^16.9.8",
    "chai": "^4.2.0",
    "husky": "^4.2.5",
    "mocha": "^7.2.0",
    "np": "^6.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devim-front/route.git"
  },
  "bugs": {
